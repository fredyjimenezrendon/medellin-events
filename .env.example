# Redis Configuration (Upstash)
UPSTASH_REDIS_REST_URL=your_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_redis_token_here

# Admin Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD_HASH_BASE64=your_bcrypt_hash_base64_encoded_here

# Session Secret (generate a random string)
SESSION_SECRET=your_random_secret_key_here_min_32_chars

# Google Maps API Key (for interactive location picker)
# Get your key at: https://console.cloud.google.com/google/maps-apis
# Enable: Maps JavaScript API and Places API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
